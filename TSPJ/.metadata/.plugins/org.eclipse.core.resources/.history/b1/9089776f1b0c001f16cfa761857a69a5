package packShape;

public class HandleShape {

	public static void main(String[] args) {
		Shape ob1 = new Shape();
		ob1.show();
		Shape ob2 = new Shape();
		ob2.show();
		Square ob3 = new Square(7);
		ob3.show();
		Rectangle ob4 = new Rectangle(5, 10);
		ob4.show();
		Circle ob5 = new Circle(12);
		ob5.show();
		Oval ob6 = new Oval(6, 9);
		ob6.show();
	}

}
