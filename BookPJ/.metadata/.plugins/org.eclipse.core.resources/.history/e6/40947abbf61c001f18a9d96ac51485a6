package packScore;

import java.util.StringTokenizer;

public class Score implements InterScore {
	private String No; //응시번호
	private int[] sc; //성적배열
	Score(String No){
		this.No = No;
		sc = new int[many];
	}
	Score(String No, int size){
		this.No = No;
		sc = new int[size];
	}
	public String getNo() { //이식받은 매서드의 접근범위 줄일수 없다
		return No;
	}
	public boolean putScore(String txt) {
		StringTokenizer st = new StringTokenizer(txt,",");
		for(int x:sc) {
			x = Integer.parseInt(st.nextToken().trim());
		}
		
	}
}
